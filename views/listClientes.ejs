<!DOCTYPE html>
<html lang="pt-br">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <base href="/" />
    <link rel="stylesheet" href="css/style.css" />
    <title>Banco de dados</title>
  </head>
  <body>
    <div class="main-container">
      <h1 class="page-title">Projeto BeMobile</h1>
      <a href="/registro/clientes" id="register-button">Novo Cliente</a>
      <li class="clientes-lista">
        <ul class="legenda-infos">
          <div><h2>ID</h2></div>
          <div><h2>Nome</h2></div>
          <div><h2>Cidade</h2></div>
        </ul>
        <% dados.forEach(element => { %>
        <ul class="cliente-infos">
          <div><%= element.idClientes %></div>
          <div><%= element.nomeClientes %></div>
          <div><%= element.Cidade %></div>
          <button>
            <a href="/view/clientes/<%= element.idClientes %>">Detalhes</a>
          </button>
        </ul>
        <% }) %>
      </li>
      <a
        href="/index"
        id="register-button"
        style="align-items: center; justify-content: center; display: flex"
        >PÃ¡gina Inicial</a
      >
    </div>
  </body>
  <script src="../public/js/script.js"></script>
</html>
